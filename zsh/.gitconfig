[user]
	email = lancearevalo2000@gmail.com
	name = arevalolance
[credential]
	helper = cache --timeout=3600
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[core]
  pager = delta
[interactive]
  diffFilter = delta --color-only
[add.interactive]
  useBuiltin = false
[delta]
  navigate = true
  light = false
  side-by-side = true
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
